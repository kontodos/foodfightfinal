<h1>List of your reviews.</h1>
<table class="table">
	<thead>
		<tr>
			<td>Id</td>
			<td>Name</td>
			<td>Content</td>
			<td>Posted On</td>
		</tr>
	</thead>
	
	<tbody>
	<% @user.reviews.each do |f| %>
	<tr>
		<td><%= f.id %></td>

		<td><%= f.food.name %></td>
		<td><%= f.content %></td>
		<td><%= f.posted_on %></td>
		
		
	</tr>
	<% end %>
	</tbody>
	</table>
	
